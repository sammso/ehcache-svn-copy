<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://appengine.google.com/ns/1.0 http://googleappengine.googlecode.com/svn/branches/1.2.1/java/docs/appengine-web.xsd">
    <application>${gae.application.name}</application>
    <!--No automated way of deleting a version. GAE only allows 10. Used a fixed
        version until the GAE plugin supports deleting. See http://code.google.com/p/maven-gae-plugin/issues/detail?id=94-->
    <!--<version>${buildNumber}</version>-->
    <version>10000</version>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
    </system-properties>

    <sessions-enabled>true</sessions-enabled>

</appengine-web-app>
