<?xml version="1.0" encoding="UTF-8"?>

<ehcache>

    <defaultCache
            maxElementsInMemory="10"
            eternal="false"
            timeToIdleSeconds="5"
            timeToLiveSeconds="10"
            overflowToDisk="false"
            />


    <cache name="txCache1"
           maxElementsInMemory="10"
           eternal="false"
           timeToIdleSeconds="5"
           timeToLiveSeconds="10"
           transactionalMode="non_xa"
           />

    <cache name="txCache2"
           maxElementsInMemory="10"
           eternal="false"
           timeToIdleSeconds="5"
           timeToLiveSeconds="10"
           transactionalMode="non_xa"
           />
</ehcache>
